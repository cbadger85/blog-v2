{
  "name": "blog",
  "author": "Charles Badger",
  "version": "0.0.1",
  "description": "My blog, yo",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "npm rum build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "vite build --mode ssr --ssr src/server.tsx",
    "dev": "ts-node --esm generator.ts",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"./src/**/*.{ts,tsx}\" --fix",
    "serve": "vite preview --port 3000",
    "start": "npm run dev",
    "test": "vitest --mode test",
    "test:coverage": "vitest run --mode test --coverage",
    "typecheck": "tsc"
  },
  "license": "MIT",
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.0.1",
    "@testing-library/user-event": "^14.1.0",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.26",
    "@types/react": "^18.0.1",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "@vitejs/plugin-react": "^1.3.0",
    "c8": "^7.11.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.13.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "express": "^4.17.3",
    "jsdom": "^19.0.0",
    "prettier": "^2.6.2",
    "serve-static": "^1.15.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "typescript-plugin-css-modules": "^3.4.0",
    "vite": "^2.9.1",
    "vite-plugin-checker": "^0.4.5",
    "vite-tsconfig-paths": "^3.4.1",
    "vitest": "^0.9.3"
  }
}
