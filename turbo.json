{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "start": {
      "cache": false,
      "dependsOn": ["dev"]
    },
    "serve": {
      "dependsOn": ["build"]
    },
    "dev": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".generator/**"]
    },
    "test": {
      "outputs": [],
      "inputs": ["src/**/*.tsx", "src/**/*.ts"]
    },
    "test:coverage": {
      "outputs": [],
      "inputs": ["src/**/*.tsx", "src/**/*.ts"]
    },
    "typecheck": {
      "outputs": [],
      "inputs": ["**/*.tsx", "**/*.ts"]
    },
    "lint": {
      "outputs": []
    },
    "lint:fix": {
      "outputs": []
    }
  }
}
